                <GroupBox x:Name="grpx81"  HorizontalAlignment="Left" Margin="3,10850,0,0" VerticalAlignment="Top"  Width="627" Grid.ColumnSpan="2" BorderThickness="0">
                    <Grid Margin="3,0">
