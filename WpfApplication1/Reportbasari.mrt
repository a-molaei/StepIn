        <Text7 Ref="4" type="Text" isKey="true">
          <Brush>Transparent</Brush>
          <ClientRectangle>0.2,7.8,5.3,3.5</ClientRectangle>
          <Conditions isList="true" count="0" />
          <Font>B Zar,11</Font>
          <Guid>3a0c23a0c4ce41e4bb285aa7b37802dd</Guid>
