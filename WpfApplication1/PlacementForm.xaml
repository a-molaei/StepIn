                <GroupBox x:Name="grpx28"  HorizontalAlignment="Stretch"  FontFamily="Times New Roman" FontSize="16" Margin="3,6700,0,0" VerticalAlignment="Top"   Grid.ColumnSpan="2" BorderThickness="0">
                    <Grid Margin="3,0"  HorizontalAlignment="Stretch">
